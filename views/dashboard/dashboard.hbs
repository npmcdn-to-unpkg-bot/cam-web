<div id="dashboard" data-component="dashboard">

  <div id="family-badges">
    {{#each users}}
      {{> user-badge/user-badge}}
    {{/each}}
  </div>


  <div data-component="button-group">
    <button class="active">Active Missions</button>
    <button>Completed Missions</button>
  </div>

  {{> mission-card/mission-card class="create"}}
  <div id="active-goals">
    {{#each missions}}
      {{> mission-card/mission-card users=../users}}
    {{/each}}
  </div>
</div>
